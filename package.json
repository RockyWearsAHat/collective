{
  "name": "cookie-place",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --raw --prefix none \"npm run build-css\" \"vite\"",
    "build": "tsc && vite build && rollup -c rollup.config.mjs",
    "build-css": "tailwindcss -i ./pages/tailwindInput.css -o ./pages/tailwindOutput.css --watch",
    "buildnrun": "npm run removeBuild && npm run build && npm run start",
    "removeBuild": "rm -rf build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "node build/server/server.js"
  },
  "dependencies": {
    "@react-spring/parallax": "^9.7.3",
    "bcrypt": "^5.1.1",
    "express": "^4.18.3",
    "js-cookie": "^3.0.5",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@hono/vite-dev-server": "^0.8.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.10",
    "magic-string": "^0.30.8",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "rollup": "^4.12.0",
    "rollup-plugin-gzip": "^3.1.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-3d": "^1.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
