{
  "name": "cookie-place",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "email": "email dev",
    "dev": "concurrently -r \"npm run build-css\" \"vite --host\"",
    "stripe": "stripe listen --forward-to localhost:4000/api/checkout/stripeWebhooks",
    "build": "tsc && vite build && rollup -c rollup.config.mjs",
    "build-css": "tailwindcss -c tailwind.config.mjs -i ./pages/tailwindInput.css -o ./pages/tailwindOutput.css --watch",
    "buildnrun": "npm run removeBuild && npm run build && npm run start",
    "removeBuild": "rm -rf build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "node build/server/server.js",
    "installSharp": "npm install --cpu=x64 --os=linux --libc=glibc sharp && npm install --cpu=x64 --os=linux --libc=musl sharp"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.620.1",
    "@aws-sdk/client-ses": "^3.623.0",
    "@aws-sdk/s3-request-presigner": "^3.620.1",
    "@imgly/background-removal": "^1.5.3",
    "@react-email/components": "0.0.22",
    "@react-spring/parallax": "^9.7.3",
    "@stripe/react-stripe-js": "^2.7.3",
    "@stripe/stripe-js": "^4.1.0",
    "bcrypt": "^5.1.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2",
    "rollup-plugin-gzip": "^3.1.2",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.5",
    "stripe": "^16.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-mongo": "^3.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "glob": "^10.3.12",
    "magic-string": "^0.30.10",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rollup": "^4.14.3",
    "tailwindcss": "^3.4.3",
    "tailwindcss-cli": "^0.1.2",
    "tsx": "^4.7.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
