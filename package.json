{
  "name": "cookie-place",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -r \"npm run build-css\" \"vite --host\"",
    "build": "tsc && vite build && rollup -c rollup.config.mjs",
    "build-css": "tailwindcss -c tailwind.config.mjs -i ./pages/tailwindInput.css -o ./pages/tailwindOutput.css --watch",
    "buildnrun": "npm run removeBuild && npm run build && npm run start",
    "removeBuild": "rm -rf build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "node build/server/server.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/s3-request-presigner": "^3.574.0",
    "@react-spring/parallax": "^9.7.3",
    "bcrypt": "^5.1.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cookie-session": "^2.0.48",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.12",
    "magic-string": "^0.30.8",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rollup": "^4.14.3",
    "rollup-plugin-gzip": "^3.1.2",
    "tailwindcss": "^3.4.3",
    "tailwindcss-cli": "^0.1.2",
    "tsx": "^4.7.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-mkcert": "^1.17.5"
  }
