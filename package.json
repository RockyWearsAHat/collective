{
  "name": "cookie-place",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --raw --prefix none \"npm run build-css\" \"vite\"",
    "build": "tsc && vite build && rollup -c rollup.config.mjs",
    "build-css": "tailwindcss -i ./pages/tailwindInput.css -o ./pages/tailwindOutput.css --watch",
    "buildnrun": "npm run build && npm run start",
    "removeBuild": "rm -rf build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.1",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "hono": "^4.0.7",
    "mongoose": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@hono/vite-dev-server": "^0.8.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.10",
    "parallelshell": "^3.0.2",
    "rollup": "^4.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
